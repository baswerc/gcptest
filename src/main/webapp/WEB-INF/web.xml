<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>GCP Test</display-name>

  <description>GCP Test</description>

  <listener>
    <listener-class>gov.nasa.gcptest.Bootstrap</listener-class>
  </listener>


  <servlet>
    <servlet-name>PropertiesServlet</servlet-name>
    <servlet-class>gov.nasa.gcptest.PropertiesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PropertiesServlet</servlet-name>
    <url-pattern>/props</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ErrorServlet</servlet-name>
    <servlet-class>gov.nasa.gcptest.ErrorServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ErrorServlet</servlet-name>
    <url-pattern>/error</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>PrimesServlet</servlet-name>
    <servlet-class>gov.nasa.gcptest.PrimesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PrimesServlet</servlet-name>
    <url-pattern>/primes</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Trace1Servlet</servlet-name>
    <servlet-class>gov.nasa.gcptest.Trace1Servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Trace1Servlet</servlet-name>
    <url-pattern>/trace1</url-pattern>
  </servlet-mapping>
</web-app>